# Enhanced Termux/Android Environment Configuration
# Generated automatically for Termux 0.119.0-beta.3 environment
# Based on provided environment variables for optimal compatibility

# Termux Environment Detection
TERMUX_DETECTED=true
TERMUX_VERSION="0.119.0-beta.3"
TERMUX_PACKAGE_NAME="com.termux"
TERMUX_HOME="/data/data/com.termux/files/home"
TERMUX_PREFIX="/data/data/com.termux/files/usr"
TERMUX_ROOTFS="/data/data/com.termux/files"

# Android Environment
ANDROID_RUNTIME_ROOT="/apex/com.android.runtime"
EXTERNAL_STORAGE="/sdcard"
TARGET_SDK_VERSION=28

# Java Configuration
JAVA_HOME="/data/data/com.termux/files/usr/lib/jvm/java-17-openjdk/"

# Shell and Language
SHELL="/data/data/com.termux/files/usr/bin/bash"
LANG="en_US.UTF-8"
COLORTERM="truecolor"

# VARIABOT Optimizations for Termux
VARIABOT_TERMUX=true
VARIABOT_MOBILE_OPTIMIZED=true
VARIABOT_MEMORY_LIMIT="1.5GB"
VARIABOT_CPU_THREADS=3
VARIABOT_PERFORMANCE_PROFILE="termux_optimized"
VARIABOT_MODEL_SIZE="small"
VARIABOT_UI_MODE="mobile"
VARIABOT_STORAGE_PATH="/data/data/com.termux/files/home/.variabot"
VARIABOT_TEMP_PATH="/data/data/com.termux/files/home/tmp"
VARIABOT_CACHE_PATH="/data/data/com.termux/files/home/.variabot/cache"

# Python Optimizations
PYTHONOPTIMIZE=1
PYTHONUNBUFFERED=1
PYTHONPATH="/data/data/com.termux/files/usr/lib/python3.10/site-packages"
TMPDIR="/data/data/com.termux/files/home/tmp"

# Streamlit Mobile Optimizations
STREAMLIT_SERVER_PORT=8080
STREAMLIT_SERVER_ADDRESS="127.0.0.1"
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false
STREAMLIT_SERVER_FILE_WATCHER_TYPE="none"
STREAMLIT_THEME_BASE="dark"
STREAMLIT_SERVER_ENABLE_CORS=false
STREAMLIT_SERVER_ENABLE_XSRF_PROTECTION=true
STREAMLIT_SERVER_HEADLESS=true
STREAMLIT_SERVER_MAX_UPLOAD_SIZE=50

# Mobile UI Optimizations
STREAMLIT_THEME_PRIMARY_COLOR="#007bff"
STREAMLIT_THEME_BACKGROUND_COLOR="#1e1e1e"
STREAMLIT_THEME_SECONDARY_BACKGROUND_COLOR="#2a2a2a"
STREAMLIT_THEME_TEXT_COLOR="#ffffff"

# Security and Performance
LD_PRELOAD="/data/data/com.termux/files/usr/lib/libtermux-exec-ld-preload.so"
HISTCONTROL="ignoreboth"

# AI Model Constraints for Mobile
TRANSFORMERS_CACHE="/data/data/com.termux/files/home/.variabot/cache/transformers"
HUGGINGFACE_HUB_CACHE="/data/data/com.termux/files/home/.variabot/cache/huggingface"
TORCH_HOME="/data/data/com.termux/files/home/.variabot/cache/torch"

# Resource Limits
PYTORCH_CUDA_ALLOC_CONF="max_split_size_mb:512"
TOKENIZERS_PARALLELISM=false
OMP_NUM_THREADS=2

# Network Configuration
HTTP_PROXY=""
HTTPS_PROXY=""
NO_PROXY="localhost,127.0.0.1"